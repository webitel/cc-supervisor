{
  "name": "cc-supervisor",
  "version": "22.01.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/supervisor:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "lint": "vue-cli-service lint --fix",
    "link-local-webitel-ui": "rm -rf node_modules/@webitel/ui-sdk && ln -s /home/user/Projects/webitel-ui-sdk node_modules/@webitel/ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "prepare": "husky install"
  },
  "dependencies": {
    "@webitel/cc-ui-sdk": "^0.1.21",
    "@webitel/ui-sdk": "^1.0.12",
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "csv-stringify": "^5.5.1",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "file-saver": "^2.0.2",
    "jsdom": "^16.4.0",
    "svg-sprite-loader": "^5.0.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vue-multiselect": "^2.1.6",
    "vue-native-websocket": "^2.0.14",
    "vue-router": "^3.1.6",
    "vue-select": "^3.10.2",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.3",
    "webitel-sdk": "^0.1.83"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "husky": "^7.0.0"
  }
}
