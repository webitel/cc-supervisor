    "@vuelidate/validators": "^2.0.4",
    "@webitel/api-services": "^0.0.56",
    "@webitel/ui-datalist": "^1.0.78",
    "@webitel/ui-sdk": "~25.10",
    "axios": "^1.7.7",
    "clipboard-copy": "^4.0.1",
