{
  "name": "cc-supervisor",
  "version": "22.09.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/supervisor:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "lint": "vue-cli-service lint --fix",
    "link-local-webitel-ui": "rm -rf node_modules/@webitel/ui-sdk && ln -s /home/user/Projects/webitel-ui-sdk node_modules/@webitel/ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "prepare": "husky install"
  },
  "dependencies": {
    "@webitel/cc-ui-sdk": "^0.1.25",
    "@webitel/ui-sdk": "^2.0.2",
    "axios": "^0.27.2",
    "clipboard-copy": "^4.0.1",
    "core-js": "^3.6.4",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "svg-sprite-loader": "^6.0.11",
    "vue": "^2.6.14",
    "vue-i18n": "^8.27.1",
    "vue-native-websocket": "^2.0.15",
    "vue-router": "^3.5.4",
    "vuelidate": "^0.7.7",
    "vuex": "^3.6.2",
    "webitel-sdk": "^0.1.120"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "jsdom": "^16.4.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "sass": "^1.52.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
