{
  "name": "cc-supervisor",
  "version": "23.09.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "build-and-run-locally": "npm run build && sudo docker run -p 80:80 -v ${PWD}/dist:/usr/share/nginx/html:rw -v ${HOME}/default.conf:/etc/nginx/conf.d/default.conf:ro -v ${PWD}/dist:/www/supervisor:rw -v ${HOME}/Projects/web-client-auth/dist:/www/auth:ro --rm nginx",
    "lint": "vue-cli-service lint --fix",
    "link-local-webitel-ui": "rm -rf node_modules/@webitel/ui-sdk && ln -s /home/user/Projects/webitel-ui-sdk node_modules/@webitel/ui-sdk",
    "link-local-webitel-sdk": "rm -rf node_modules/webitel-sdk && ln -s /home/user/Projects/webitel_sdk/dist node_modules/webitel-sdk",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@webitel/ui-sdk": "^23.9.7",
    "axios": "^0.27.2",
    "clipboard-copy": "^4.0.1",
    "core-js": "^3.6.4",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "webitel-sdk": "^0.1.191"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/vue3-jest": "^29.2.2",
    "bamboo-jest-reporter": "^1.0.3",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "jest-environment-jsdom": "^29.4.3",
    "jsdom": "^16.4.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "sass": "^1.52.1",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^6.0.11"
  },
  "overrides": {
    "@vue/vue3-jest": "^29.2.2",
    "vue": "^3.2.47",
    "babel-jest": "^29.*",
    "jest": "^29.*"
  }
}
