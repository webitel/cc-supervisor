{
  "name": "@webitel/supervisor",
  "version": "25.12.0",
  "private": true,
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "test:unit": "vitest",
    "lint:fix": "eslint --fix ./src && prettier --write --check ./src",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "utils:i": "npm install --install-links",
    "utils:up-libs": "npm update @webitel/ui-sdk @webitel/ui-datalist @webitel/api-services && npm run utils:ls-libs",
    "utils:link-libs": "npm link @webitel/ui-sdk @webitel/ui-datalist @webitel/api-services && npm run utils:ls-libs",
    "utils:ls-libs": "npm ls @webitel/ui-sdk @webitel/ui-datalist @webitel/api-services"
  },
  "type": "module",
  "dependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@vue/compat": "^3.5.x",
    "@vue/tsconfig": "^0.7.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@webitel/api-services": "^0.1.7",
    "@webitel/styleguide": "~26.2",
    "@webitel/ui-datalist": "=1.1.2",
    "@webitel/ui-sdk": "~26.2",
    "axios": "^1.13",
    "clipboard-copy": "^4.0.1",
    "deep-copy": "^1.x",
    "deep-equal": "^2.x",
    "deepmerge": "^4.x",
    "lodash-es": "^4.x",
    "pinia": "^3.x",
    "vue": "^3.5.x",
    "vue-i18n": "^11.x",
    "vue-router": "^4.x",
    "vue-tsc": "^2.2.8",
    "vuex": "^4.1.0",
    "webitel-sdk": "^25.10.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^6.x",
    "@vitest/coverage-v8": "^4.x",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^17.x",
    "happy-dom": "^15.7.3",
    "lint-staged": "^16.x",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prismjs": "^1.29.0",
    "sass": "^1.77.2",
    "tslib": "^2.7.0",
    "typescript-eslint": "^8.26.1",
    "vite": "npm:rolldown-vite@^7.3.x",
    "vite-plugin-node-polyfills": "^0.x",
    "vite-plugin-vue-devtools": "^8.x",
    "vite-plugin-vue-docgen": "^0.3.4",
    "vitepress": "^1.x",
    "vitest": "^4.x"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.24.0",
    "@rollup/rollup-linux-x64-gnu": "^4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@^7.3.x"
  }
}
